/* S3 path of the folder containing jenkins jobs */
/* Leave jenkins_jobs_s3_path value as empty string if no jobs to restore(jenkins_jobs_s3_path = "")  */
jenkins_jobs_s3_path = "bucket_name/folder_name/sub_folder_containing_jenkins_jobs"
restore_jenkins = "0"   // Enable restore- 1, Disable restore - 0
jenkins_backup_s3_path = "bucket_name/folder_name/sub_folder_containing_jenkins_jobs"
